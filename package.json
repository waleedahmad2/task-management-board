{
  "name": "project-boilerplate",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "/app",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "serve dist",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "svgs": "npx @svgr/cli -d src/assets/svgs/components src/assets/svgs --filename-case kebab",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run src/test/unit",
    "test:integration": "vitest run src/test/integration",
    "test:unit:watch": "vitest --watch src/test/unit",
    "test:integration:watch": "vitest --watch src/test/integration",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write --config ./.prettierrc",
      "eslint --fix --config ./eslint.config.ts"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/react": "^9.11.0",
    "@sentry/vite-plugin": "^3.2.2",
    "@tailwindcss/vite": "^4.1.2",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-query-devtools": "^5.71.5",
    "axios": "^1.8.4",
    "change-object-case": "^0.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.543.0",
    "mock-socket": "^9.3.1",
    "moment": "^2.30.1",
    "msw": "^2.11.1",
    "react": "^19.1.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.56.4",
    "react-router-dom": "^7.4.1",
    "react-toastify": "^11.0.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.2",
    "zod": "^3.25.13"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-define-config": "^2.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^16.0.0",
    "prettier": "^3.6.2",
    "serve": "^14.2.4",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.5",
    "vitest": "^2.1.8"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}

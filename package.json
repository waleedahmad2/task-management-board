{
  "name": "project-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "/app",
  "scripts": {
    "dev": "vite",
    "generate-meta-tag": "node ./node_modules/react-cache-buster/dist/generate-meta-tag.js",
    "build": "yarn generate-meta-tag && vite build",
    "serve": "serve dist",
    "lint": "eslint 'src/**/*.{js,jsx}' --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install",
    "svgs": "npx @svgr/cli -d src/assets/svgs/components src/assets/svgs --filename-case kebab"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@sentry/react": "^8.45.0",
    "@sentry/vite-plugin": "^2.22.7",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-query-devtools": "^5.62.7",
    "axios": "^1.7.9",
    "change-object-case": "^0.2.1",
    "crypto-js": "^4.2.0",
    "formik": "^2.4.6",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^19.0.0",
    "react-cache-buster": "^0.1.8",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.2",
    "react-toastify": "^10.0.6",
    "yup": "^1.5.0"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "eslint": "^9.16.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "3.4.2",
    "serve": "^14.2.4",
    "vite": "^6.0.3"
  }
}
